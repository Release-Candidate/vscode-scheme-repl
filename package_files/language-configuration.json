{
    "comments": {
        "blockComment": [
            "#|",
            "|#"
        ],
        "lineComment": ";;"
    },
    "brackets": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "autoClosingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "#|",
            "|#"
        ],
        [
            {
                "open": "\"",
                "close": "\"",
                "notIn": [
                    "string"
                ]
            }
        ]
    ],
    "surroundingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "colorizedBracketPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "wordPattern": "[^;\"'`(\\[{\\s)\\]}]+",
    "onEnterRules": [
        {
            "beforeText": "^\\s*\\(\\s*(define|if\\s|lambda\\s|import\\s|do\\s|unless\\s|for-each\\s).*$",
            "action": {
                "indent": "indent"
            }
        },
        {
            "beforeText": "^\\s*;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";"
            }
        },
        {
            "beforeText": "^\\s*;;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";;"
            }
        },
        {
            "beforeText": "^\\s*;;;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";;;"
            }
        },
        {
            "beforeText": "^\\s*;;;;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";;;;"
            }
        },
    ]
}
